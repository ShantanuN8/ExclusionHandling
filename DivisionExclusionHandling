#include<iostream>
using namespace std;
int main(){
    int a,b,c;
    cout<<"Enter two Numbers";
    cin>>a>>b;
    try{
        if(b!=0){
            c=a/b;
            cout<<"The answer is "<<c<<endl;
        }
        else{
            throw(b);
        }
    }
    catch(int b){
        cout<<"Error! Value of b is incorrect";}
}
